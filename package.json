{
  "name": "conversationalflowsdesigner",
  "version": "1.0.0",
  "description": "Conversational Flows Designer",
  "private": true,
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll"
  },
  "dependencies": {
    "@stencil/core": "^1.7.2",
    "@stencil/router": "^1.0.1",
    "chokidar": "^3.2.2"
  },
  "devDependencies": {
    "@stencil/sass": "^1.1.0",
    "@types/jest": "24.0.18",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genexuslabs/Conversational-Flow-Designer.git"
  },
  "author": "GeneXus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genexuslabs/Conversational-Flow-Designer/issues"
  },
  "homepage": "https://github.com/genexuslabs/Conversational-Flow-Designer#readme",
  "jest": {
    "transform": {
      "^.+//.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|//.(test|spec))//.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
