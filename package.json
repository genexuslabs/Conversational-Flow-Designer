{
  "name": "@genexuslabs/conversational-flows-designer",
  "version": "17.0.34",
  "description": "Conversational Flows Designer",
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "build": "stencil build",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "validate": "npm run lint && npm run build --prod",
    "patch": "npm version patch",
    "pushWithTags": "git push --follow-tags"
  },
  "devDependencies": {
    "@genexus/gemini": "0.1.157",
    "@stencil/core": "^1.17.3",
    "@stencil/router": "^1.0.1",
    "@stencil/sass": "^1.3.2",
    "@types/jest": "^24.9.1",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chokidar": "^3.4.2",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.1",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genexuslabs/Conversational-Flow-Designer.git"
  },
  "author": "GeneXus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genexuslabs/Conversational-Flow-Designer/issues"
  },
  "homepage": "https://github.com/genexuslabs/Conversational-Flow-Designer#readme",
  "jest": {
    "transform": {
      "^.+//.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|//.(test|spec))//.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{tsx,ts,jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
