{
  "name": "conversationalflowsdesigner",
  "version": "1.0.0",
  "description": "Conversational Flows Designer",
  "private": true,
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "lint": "eslint ./src/**/*.ts ./src/**/*.tsx",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "validate": "npm run lint && npm run build && npm run test"
  },
  "dependencies": {
    "@stencil/core": "^1.7.4",
    "@stencil/router": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "chokidar": "^3.2.2",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.9",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2"
  },
  "devDependencies": {
    "@stencil/sass": "^1.1.0",
    "@types/jest": "24.0.18",
    "workbox-build": "4.3.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genexuslabs/Conversational-Flow-Designer.git"
  },
  "author": "GeneXus",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genexuslabs/Conversational-Flow-Designer/issues"
  },
  "homepage": "https://github.com/genexuslabs/Conversational-Flow-Designer#readme",
  "jest": {
    "transform": {
      "^.+//.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|//.(test|spec))//.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{tsx,ts,jsx,js,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.{tsx,ts,jsx,js}": [
      "eslint --fix",
      "git add"
    ]
  }
}
